{
	"version": "2",
	"templates": [
		{
			"categories": [
				"Other",
				"Tools"
			],
			"description": "Passbolt is a free and open source password manager designed for collaboration. With Passbolt you can securely generate, store, manage and monitor your team credentials. Get access to all of your logins and passwords from multiple browsers or even your mobile phone.",
			"env": [
				{
					"default": "1000",
					"label": "MariaDB PUID",
					"name": "PUID"
				},
				{
					"default": "1000",
					"label": "MariaDB PGID",
					"name": "PGID"
				},
				{
					"default": "Europe/Zurich",
					"label": "MariaDB timezone",
					"name": "TZ"
				},
				{
					"default": "wfNJYSFmqTj3",
					"label": "MariaDB root password",
					"name": "MYSQL_ROOT_PASSWORD"
				},
				{
					"default": "passbolt",
					"label": "Passbolt database",
					"name": "MYSQL_DATABASE"
				},
				{
					"default": "passbolt",
					"label": "Passbolt database user",
					"name": "MYSQL_USER"
				},
				{
					"default": "J5cT92cjAErz",
					"label": "Passbolt database password",
					"name": "MYSQL_PASSWORD"
				},
				{
					"default": "https://passbolt.mydomain.com",
					"label": "Passbolt full HTTPS base URL",
					"name": "APP_FULL_BASE_URL"
				},
				{
					"default": "443",
					"label": "Passbolt HTTPS port",
					"name": "PASSBOLT_PORT"
				}
			],
			"logo": "https://raw.githubusercontent.com/blenherr/pi-hosted-test/master/images/passbolt.png",
			"name": "Passbolt",
			"platform": "linux",
